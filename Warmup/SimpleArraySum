import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* to find the sum of elements in an array */
        Scanner scan = new Scanner(System.in);
        
        int sum=0;
        int N = scan.nextInt();
        
        for(int i=0;i < N;i++){
            sum += scan.nextInt();
        }
        
        System.out.println(sum);
        
    }
}
