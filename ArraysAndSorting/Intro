import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.Scanner;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int V = sc.nextInt();
        int n = sc.nextInt();
        
        int [] arrays = new int[n];
        
        for(int i=0;i<n;i++){
            arrays[i]= sc.nextInt();
            if(arrays[i]==V)
                System.out.println(i);
        }
        
        
    }
}
